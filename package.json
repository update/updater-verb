{
  "name": "updater-verb",
  "description": "Update a .verb.md file to use the latest conventions and fix formatting mistakes or incompatibilities.",
  "version": "0.1.1",
  "homepage": "https://github.com/update/updater-verb",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "update/updater-verb",
  "bugs": {
    "url": "https://github.com/update/updater-verb/issues"
  },
  "license": "MIT",
  "files": [
    "index.js",
    "lib",
    "LICENSE",
    "README.md",
    "updatefile.js"
  ],
  "main": "index.js",
  "preferGlobal": true,
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "delete": "^0.3.2",
    "extend-shallow": "^2.0.1",
    "fs-exists-sync": "^0.1.0",
    "is-valid-app": "^0.2.0",
    "lazy-cache": "^2.0.1",
    "through2": "^2.0.1",
    "union-value": "^0.2.3"
  },
  "devDependencies": {
    "copy": "^0.2.3",
    "generate": "^0.9.8",
    "gulp": "^3.9.1",
    "gulp-eslint": "^3.0.1",
    "gulp-format-md": "^0.1.9",
    "gulp-istanbul": "^1.0.0",
    "gulp-middleware": "^0.1.0",
    "gulp-mocha": "^2.2.0",
    "gulp-unused": "^0.1.2",
    "mocha": "^2.5.3",
    "npm-install-global": "^0.1.2"
  },
  "keywords": [
    "fix",
    "lint",
    "linter",
    "maintain",
    "pkg",
    "plugin",
    "plugins",
    "up-to-date",
    "update",
    "update-plugin",
    "update-updater",
    "updater",
    "updaterplugin",
    "verb"
  ],
  "verb": {
    "toc": true,
    "layout": "common-minimal",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "list": [
        "update"
      ]
    },
    "reflinks": [
      "update",
      "verb",
      "verb-generate-readme"
    ],
    "lint": {
      "reflinks": true
    }
  }
}
